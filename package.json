{
  "name": "cru-payments",
  "version": "1.2.2",
  "description": "Library for validating and encrypting credit card and bank account info",
  "main": "dist/cru-payments.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack -p",
    "build:analyze": "webpack -p --env.analyze",
    "lint": "eslint 'src/**'",
    "lint:ts": "tsc",
    "test": "jest",
    "test:w": "jest --watch",
    "prepublish": "yarn build",
    "prettier:check": "prettier '{src/**/*.{ts,js,json},./*.{ts,js,json,yml}}' --list-different",
    "prettier:write": "prettier '{src/**/*.{ts,js,json},./*.{ts,js,json,yml}}' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CruPCI/cru-payments.git"
  },
  "keywords": [
    "cru",
    "payments",
    "tsys"
  ],
  "author": "Scotty Waggoner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/CruPCI/cru-payments/issues"
  },
  "homepage": "https://github.com/CruPCI/cru-payments#readme",
  "dependencies": {
    "jsencrypt": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/fetch-mock": "^7.3.0",
    "@types/jest": "^24.0.13",
    "@types/mockdate": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "eslint": "^5.16.0",
    "eslint-plugin-jest": "^22.6.4",
    "husky": "^2.4.1",
    "jest": "^24.8.0",
    "jest-fetch-mock": "^2.1.2",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "mockdate": "^2.0.2",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.0",
    "ts-loader": "^6.0.2",
    "typescript": "^3.5.1",
    "typescript-eslint-parser": "^22.0.0",
    "webpack": "^4.34.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
